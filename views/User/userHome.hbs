	<script>
		function addToCart(proId) {
			$.ajax({
				url: '/addtocart/' + proId,
				method: 'get',
				success: (response) => {
					if (response.status) {
						let count = $('#cart-count').html()
						count = parseInt(count) + 1
						$('#cart-count').html(count)

						Swal.fire({
							icon: 'success',
							title: 'Item added to Cart successfully',
							showConfirmButton: false,
							timer: 1000
						})

					} else if (response.stock) {
						Swal.fire({
							icon: 'error',
							title: "Oops",
							text: 'Out Of Stocks',
							showConfirmButton: true,
						})

					}else{

                        location.href="/login"


                     }
				}
			})
		}


		function addToWhishlist(proId) {
			$.ajax({
				url: '/addtowhishlist/' + proId,
				method: 'get',
				success: (response) => {
					if (response.status) {
						let count = $('#whishlist-count').html()
						count = parseInt(count) + 1
						$('#whishlist-count').html(count)

						Swal.fire({
							icon: 'success',
							title: 'Item added to WhishList',
							showConfirmButton: false,
							timer: 1000
						})

					} else{
						location.href="/login"
					}
				}
			})
		}

	</script>

	<script>

		function Search() {

			let Input = document.getElementById('myInput').value

			$.ajax({
				url: "/search",
				method: "post",
				data: {
					Input
				},
				success: (response) => {

					if (response.status) {
						location.href = "/search/"+ Input
					}


				}

			})

		}


	</script>





